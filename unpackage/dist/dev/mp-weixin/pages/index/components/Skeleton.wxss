.custom-nav-wrap {
  position: absolute;
  top: 20rpx;
  left: 0;
  width: 750rpx;
  z-index: 9999;
}
.custom-nav-wrap .custom-nav {
  padding: 0 16px;
  width: 100%;
  height: 100%;
  box-sizing: border-box;
  display: flex;
  align-items: center;
  justify-content: space-between;
  padding-right: var(--138299a4);
}
.nav-left {
  width: 40px;
  height: 100%;
  display: flex;
  align-items: center;
  justify-content: center;
}
.nav-left-addr {
  height: 100%;
  display: flex;
  align-items: center;
  justify-content: center;
  gap: 5rpx;
  line-height: 1;
  font-size: 28rpx;
  color: #333;
}
.nav-left-addr image {
  width: 28rpx;
  height: 28rpx;
  display: block;
}
.nav-title {
  font-size: 18px;
  font-weight: 500;
  position: absolute;
  transform: translateX(-50%);
  left: 50%;
  top: var(--378138fe);
  transform: translateX(-50%);
  text-align: center;
  width: 120px;
  white-space: nowrap;
  overflow: hidden;
  text-overflow: ellipsis;
}
.nav-right {
  height: 100%;
  display: flex;
  align-items: center;
  justify-content: center;
  padding: 0 8px;
}
.content {
  min-height: 100vh;
  background: #F7F8FA;
  width: 100vw;
}
.content .head image {
  width: 750rpx;
  display: block;
}
.content .search {
  margin: 0 28rpx;
  position: relative;
}
.content .search_content {
  gap: 12rpx;
  box-sizing: border-box;
  padding: 0 20rpx;
  width: 100%;
  position: absolute;
  top: -62rpx;
  height: 124rpx;
  background-color: rgba(255, 255, 255, 0.85);
  border-radius: 16rpx;
  display: flex;
  align-items: center;
}
.content .search_input {
  flex: 1;
}
.content .search .chart {
  width: 48rpx;
  height: 48rpx;
  background-image: url(data:image/png;base64,);
  background-size: 100% 100%;
}
.content .main {
  padding: 0 28rpx;
  display: flex;
  flex-direction: column;
  gap: 26rpx;
}
.content .main .nav_menu {
  display: flex;
  justify-content: space-around;
  margin-top: 86rpx;
  border-radius: 16rpx;
  padding: 26rpx 0;
  background: #FFFFFF;
}
.content .main .nav_menu .item {
  display: flex;
  flex-direction: column;
  align-items: center;
  gap: 20rpx;
}
.content .main .nav_menu .item image {
  width: 58rpx;
  height: 58rpx;
}
.content .main .nav_menu .item text {
  font-family: Source Han Sans;
  font-size: 28rpx;
  font-weight: 350;
  color: #1A1A1A;
}
.content .main .type_card {
  height: 180rpx;
  border-radius: 16rpx;
  background: #FFFFFF;
  display: flex;
  overflow: hidden;
}
.content .main .type_card .type {
  width: 160rpx;
  height: 100%;
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
  background: #eee;
}
.content .main .type_card .type .title {
  font-family: Source Han Sans;
  font-size: 28.84rpx;
  font-weight: bold;
  line-height: normal;
  color: #000000;
}
.content .main .type_card .type .tips {
  font-size: 19.23rpx;
  padding: 6rpx 0 12rpx;
}
.content .main .type_card .type .btn {
  width: 120rpx;
  height: 28rpx;
  display: flex;
  align-items: center;
  justify-content: center;
  background-color: #FFC162;
  border-radius: 40rpx;
  font-size: 20rpx;
  color: #fff;
  background: #FFC162;
}
.content .main .type_card .list {
  height: 100%;
  flex: 1;
  display: grid;
  grid-template-columns: repeat(4, 1fr);
  grid-template-rows: repeat(2, auto);
  gap: 16rpx;
  padding: 20rpx 16rpx;
  box-sizing: border-box;
}
.content .main .type_card .list text {
  padding: 10rpx 0;
  text-align: center;
  color: #333;
  font-size: 24rpx;
  display: flex;
  align-items: center;
  justify-content: center;
  background-color: #F5F5F5;
  border-radius: 16rpx;
}
.content .main .type_card .list text.active {
  background: #E0EFFF;
  color: #1677FF;
}
.content .main .doctors {
  border-radius: 16rpx;
  background: #FFF;
  padding: 16rpx 20rpx 0 0;
}
.content .main .doctors .title {
  font-size: 32rpx;
  color: #000;
  padding: 20rpx 24rpx;
}
.content .main .doctors_content {
  display: flex;
  height: 424rpx;
  gap: 20rpx;
}
.content .main .doctors_content .left {
  flex: 1;
  display: flex;
  flex-direction: column;
  justify-content: space-between;
  background: linear-gradient(180deg, #D6E6F8 0%, #FFFFFF 75%);
  border-radius: 20rpx;
}
.content .main .doctors_content .left .info {
  text-align: center;
  color: #333;
  padding-top: 26rpx;
  line-height: 1.6;
}
.content .main .doctors_content .left .info .name {
  font-size: 32rpx;
  font-weight: bold;
}
.content .main .doctors_content .left .info .award {
  font-size: 28rpx;
}
.content .main .doctors_content .left .avator {
  display: flex;
  justify-content: flex-end;
}
.content .main .doctors_content .left .avator image {
  width: 226rpx;
  height: 296rpx;
  display: block;
}
.content .main .doctors_content .right {
  width: 362rpx;
  display: flex;
  flex-direction: column;
  gap: 12rpx;
}
.content .main .doctors_content .right .item {
  height: 133.33rpx;
  background: linear-gradient(180deg, #D6E6F8 0%, #FFFFFF 75%);
  display: flex;
  justify-content: space-between;
  align-items: center;
  border-radius: 20rpx;
}
.content .main .doctors_content .right .item_info {
  display: flex;
  flex-direction: column;
  color: #000;
  gap: 8rpx;
  padding-left: 20rpx;
}
.content .main .doctors_content .right .item_info .name {
  font-size: 30rpx;
  font-weight: 500;
}
.content .main .doctors_content .right .item_info .award {
  font-size: 20rpx;
}
.content .main .doctors_content .right .item_hd image {
  width: 142rpx;
  height: 122rpx;
  display: block;
}
.content .main .category .tab_content {
  display: flex;
  flex-direction: column;
  gap: 20rpx;
  padding: 20rpx 0;
}
.content .main .category .tab_content .iformation {
  padding: 20rpx;
  border-radius: 16rpx;
  background-color: #fff;
}
.content .main .category .tab_content .iformation .basic {
  display: flex;
  gap: 20rpx;
}
.content .main .category .tab_content .iformation .basic .avator {
  width: 140rpx;
  height: 140rpx;
  border-radius: 50%;
  overflow: hidden;
}
.content .main .category .tab_content .iformation .basic .avator image {
  width: 100%;
  height: 100%;
  display: block;
}
.content .main .category .tab_content .iformation .sign {
  flex: 1;
  display: flex;
  flex-direction: column;
  justify-content: center;
  gap: 10rpx;
}
.content .main .category .tab_content .iformation .sign_name {
  display: flex;
  gap: 20rpx;
}
.content .main .category .tab_content .iformation .sign_name .name {
  font-size: 32rpx;
  font-weight: bold;
}
.content .main .category .tab_content .iformation .sign_name .tag {
  min-width: 236rpx;
  height: 44rpx;
  background-image: url(data:image/png;base64,);
  background-size: 236rpx 44rpx;
  background-repeat: no-repeat;
  font-size: 24rpx;
  color: #Fff;
  padding-left: 44rpx;
  display: flex;
  align-items: center;
  position: relative;
}
.content .main .category .tab_content .iformation .sign .address {
  display: flex;
}
.content .main .category .tab_content .iformation .sign .address text {
  background-color: #FD8702;
  border-radius: 10rpx;
  font-size: 24rpx;
  color: #fff;
  padding: 0 10rpx;
}
.content .main .category .tab_content .iformation .describe {
  font-size: 24rpx;
  font-weight: 350;
  line-height: 40rpx;
  color: #606972;
  padding: 20rpx 0;
}
.content .main .category .tab_content .iformation .feature {
  display: flex;
  align-items: center;
  justify-content: space-between;
}
.content .main .category .tab_content .iformation .feature .btn {
  display: flex;
  align-items: center;
  justify-content: center;
  gap: 24rpx;
}
.content .main .category .tab_content .iformation .feature .btn image {
  width: 46rpx;
  height: 46rpx;
}
.content .main .category .tab_content .iformation .feature .btn text {
  font-size: 24rpx;
  color: #333;
}
